  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/lego-auto.go",
      "args": ["run", "-c", "${workspaceFolder}/config.yaml"],
      "cwd": "${workspaceFolder}",
      "env": {
        "LA_CACHEDIR": "/tmp/output/.cache",
        "LA_DATADIR": "/tmp/output"
      }
    }
  ]
}
